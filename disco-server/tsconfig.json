{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "removeComments": false,
    "noImplicitAny": false,
    "outDir": "built/",
    "types": [
      "node", "noble"
    ]
  },
  "exclude": [
    "built",
    "node_modules"
  ]
}
